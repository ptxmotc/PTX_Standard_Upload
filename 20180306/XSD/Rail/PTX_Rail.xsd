										<xs:annotation>
											<xs:documentation>文字描述</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="FloorLevel" type="xs:string">
										<xs:annotation>
											<xs:documentation>所在樓層(一樓以上以1F,2F,…,#F；地下一樓以下者以“B”+數字，例如：B1,B2,…,B#)</xs:documentation>
										</xs:annotation>
