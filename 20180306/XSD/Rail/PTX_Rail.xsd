																			<xs:documentation>車站出入口簡圖名稱
</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="MapURL" type="xs:string">
																		<xs:annotation>
																			<xs:documentation>車站出入口簡圖網址
</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="FloorLevel" type="xs:string">
																		<xs:annotation>
																			<xs:documentation>樓層
</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RailStationFacilityType">
