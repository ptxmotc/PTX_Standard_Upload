<xs:schema xmlns="http://ptx.transportdata.tw/standard/schema/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://ptx.transportdata.tw/standard/schema/" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="2.0" id="PTX_Rail_MRT">
	<!--=======[Dependencies]=======-->
	<xs:include schemaLocation="../../PTX_Common.xsd"/>
	<!--=======[3rd Layer Rail MRT Data Type]=======-->
	<xs:include schemaLocation="../PTX_Rail_Codes.xsd"/>
	<xs:complexType name="MRTStationType">
		<xs:annotation>
			<xs:documentation>捷運車站資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StationID" type="xs:string">
				<xs:annotation>
					<xs:documentation>車站代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StationName" type="NameType">
				<xs:annotation>
					<xs:documentation>車站名稱</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StationPosition" type="PointType">
				<xs:annotation>
					<xs:documentation>車站位置</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StationAddress" type="xs:string">
				<xs:annotation>
					<xs:documentation>車站地址</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StationDescription" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>車站詳細說明描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StationPhone">
				<xs:annotation>
					<xs:documentation>車站聯絡電話</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[0][0-9]{1,2}-[0-9]{6,8}#?([0-9]*)?"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BikeAllowOnHoliday" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>假日自行車進出與否</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StationURL" type="xs:anyURI">
				<xs:annotation>
					<xs:documentation>車站相關資訊網址</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MRTLineType">
		<xs:annotation>
			<xs:documentation>捷運實體路線資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LineNo" type="xs:int">
				<xs:annotation>
					<xs:documentation>實體路線編號</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LineID" type="xs:string">
				<xs:annotation>
					<xs:documentation>實體路線代號</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LineName" type="NameType">
				<xs:annotation>
					<xs:documentation>實體路線名稱</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LineSectionName" type="NameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>實體路線起迄區間清單</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LineColor" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>實體路線代表顏色(以RGB色碼描述)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsBranch" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>是否為支線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LineGroup" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>路線群組</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LineURL" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>路線資訊說明網址</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MRTRouteType">
		<xs:annotation>
			<xs:documentation>捷運路線資料別型</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RouteID" type="xs:string">
				<xs:annotation>
					<xs:documentation>營運路線代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Operator" type="OperatorType">
				<xs:annotation>
					<xs:documentation>營運業者資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteName" type="NameType">
				<xs:annotation>
					<xs:documentation>路線名稱</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteType" type="RailRouteTypeEnum">
				<xs:annotation>
					<xs:documentation>路線類別代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Line" type="MRTLineType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>營運路線所屬之實體路線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Direction" type="DirectionEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>方向性描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartStation" type="RailStationType">
				<xs:annotation>
					<xs:documentation>起點車站</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndStation" type="RailStationType">
				<xs:annotation>
					<xs:documentation>迄點車站</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TravelTime" type="xs:double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>營運路線運行時間，以分鐘為單位</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteDistance" type="xs:double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>營運路線長度距離，以公里為單位</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MRTStationLiveBoardType">
		<xs:annotation>
			<xs:documentation>捷運站別即時到離站資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Station" type="MRTStationType">
				<xs:annotation>
					<xs:documentation>車站資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Route" type="MRTRouteType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>營運路線資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Line" type="MRTLineType">
				<xs:annotation>
					<xs:documentation>實體路線資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TripHeadSign" type="xs:string">
				<xs:annotation>
					<xs:documentation>預設為最近列車終點站描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Platform" minOccurs="0">
				<xs:annotation>
					<xs:documentation>月台描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndingStation" type="MRTStationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>終點站</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EstimateTime" type="xs:int">
				<xs:annotation>
					<xs:documentation>抵達時間預估</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UpdateTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>資料更新時間</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MRTODFareType">
		<xs:annotation>
			<xs:documentation>捷運起迄站收費資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OriginStation" type="RailStationType">
				<xs:annotation>
					<xs:documentation>起點車站資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DestinationStation" type="RailStationType">
				<xs:annotation>
					<xs:documentation>迄點車站資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TrainType" type="xs:string">
				<xs:annotation>
					<xs:documentation>列車種類</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Fares">
				<xs:annotation>
					<xs:documentation>票價收費資訊</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Fare" type="FareType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="TravelTime" type="xs:int">
				<xs:annotation>
					<xs:documentation>起迄站間乘車時間(分)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TravelDistance" type="xs:double">
				<xs:annotation>
					<xs:documentation>起迄站間乘車距離(公里到小數第三位)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MRTFrequencyType">
		<xs:annotation>
			<xs:documentation>捷運發車頻率資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Route" type="MRTRouteType">
				<xs:annotation>
					<xs:documentation>營運路線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Line" type="MRTLineType">
				<xs:annotation>
					<xs:documentation>實體路線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="ServiceDay" type="ServiceDayType">
					<xs:annotation>
						<xs:documentation>週內營運日</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SpecialDays">
					<xs:annotation>
						<xs:documentation>例外營運日</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SpacialDay" type="SpecialDayType" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="OperationTime">
				<xs:annotation>
					<xs:documentation>營運時間</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="StartTime" type="xs:string">
							<xs:annotation>
								<xs:documentation>起始時間HH:mm</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="EndTime" type="xs:string">
							<xs:annotation>
								<xs:documentation>結束時間HH:mm</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Headways">
				<xs:annotation>
					<xs:documentation>發車班距</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Headway" type="HeadwayType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>發車班距</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MRTS2STravelTimeType">
		<xs:annotation>
			<xs:documentation>捷運站間旅行時間資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Line" type="MRTLineType">
				<xs:annotation>
					<xs:documentation>實體路線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TrainType" type="RailTrainTypeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>車種描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TravelTimes">
				<xs:annotation>
					<xs:documentation>旅行時間</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TravelTime" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Sequence" type="xs:int">
										<xs:annotation>
											<xs:documentation>序號，由1開始，整數遞增</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="FromStation" type="RailStationType">
										<xs:annotation>
											<xs:documentation>運行(起)之車站</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ToStation" type="RailStationType">
										<xs:annotation>
											<xs:documentation>運行(迄)之車站</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="RunTime" type="xs:double">
										<xs:annotation>
											<xs:documentation>站間行駛時間(分鐘)</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="StopTime" type="xs:double">
										<xs:annotation>
											<xs:documentation>起站停靠時間(分鐘)</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MRTFirstLastTimeTableType">
		<xs:annotation>
			<xs:documentation>捷運首末班車資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Line" type="MRTLineType">
				<xs:annotation>
					<xs:documentation>實體路線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Station" type="MRTStationType">
				<xs:annotation>
					<xs:documentation>查詢車站</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TripHeadSign" type="xs:string">
				<xs:annotation>
					<xs:documentation>首末班車次之
目的地方向描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DestinationStaion" type="MRTStationType">
				<xs:annotation>
					<xs:documentation>目的地車站</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TrainType" type="xs:string">
				<xs:annotation>
					<xs:documentation>列車種類</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FirstTrainTime" type="xs:string">
				<xs:annotation>
					<xs:documentation>首班車時間HH:mm</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastTrainTime" type="xs:string">
				<xs:annotation>
					<xs:documentation>末班車時間HH:mm</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="ServiceDay" type="ServiceDayType">
					<xs:annotation>
						<xs:documentation>服務日</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SpecialDays">
					<xs:annotation>
						<xs:documentation>例外營運日</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SpacialDay" type="SpecialDayType" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<!--=======[ MRT Extension 2nd layer Data Type]=======-->
	<xs:complexType name="MRTStationExitType">
		<xs:annotation>
			<xs:documentation>捷運車站出入口資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Station" type="MRTStationType">
				<xs:annotation>
					<xs:documentation>所屬車站</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Exits">
				<xs:annotation>
					<xs:documentation>出入口資訊</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Exit" block="extension" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="RailExitType">
										<xs:sequence>
											<xs:element name="ExitMapURLs">
												<xs:annotation>
													<xs:documentation>車站出入口簡圖網址連結資訊
</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="ExitMapURL" maxOccurs="unbounded">
															<xs:annotation>
																<xs:documentation>車站出入口簡圖網址連結資訊
</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="MapName" type="NameType">
																		<xs:annotation>
																			<xs:documentation>車站出入口簡圖名稱
</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="MapURL" type="xs:string">
																		<xs:annotation>
																			<xs:documentation>車站出入口簡圖網址
</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																	<xs:element name="FloodLevel" type="xs:string">
																		<xs:annotation>
																			<xs:documentation>樓層
</xs:documentation>
																		</xs:annotation>
																	</xs:element>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MRTStationOfLineType">
		<xs:annotation>
			<xs:documentation>捷運實體路線站序資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Line" type="MRTLineType">
				<xs:annotation>
					<xs:documentation>實體路線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Stations" type="MRTStationSequenceListType">
				<xs:annotation>
					<xs:documentation>實體路線站序</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MRTStationSequenceType">
		<xs:annotation>
			<xs:documentation>捷運站序描述資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Sequence" type="xs:int">
				<xs:annotation>
					<xs:documentation>站序描述(從1開始，整數遞增)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Station" type="MRTStationType">
				<xs:annotation>
					<xs:documentation>車站資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CumulativeDistance" type="xs:double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>累積距離(由首站累積，單位為公里)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MRTTrainTypeType">
		<xs:annotation>
			<xs:documentation>捷運列車種類資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TrainTypeID" type="xs:string">
				<xs:annotation>
					<xs:documentation>車種代號</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TrainTypeCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>車種代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TrainTypeName" type="NameType">
				<xs:annotation>
					<xs:documentation>車種名稱</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MRTStopTimeType">
		<xs:annotation>
			<xs:documentation>捷運停靠元素資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StopSequence" type="xs:integer">
				<xs:annotation>
					<xs:documentation>停靠站序(由1開始)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Station" type="MRTStationType">
				<xs:annotation>
