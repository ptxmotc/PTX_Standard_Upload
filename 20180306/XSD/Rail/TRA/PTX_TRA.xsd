					<xs:documentation>實體路線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Stations" type="RailStationSequenceListType_TRA">
				<xs:annotation>
					<xs:documentation>實體路線站序</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RailRouteType_TRA">
		<xs:annotation>
			<xs:documentation>台鐵營運路線資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Line" type="RailLineType" maxOccurs="unbounded">
