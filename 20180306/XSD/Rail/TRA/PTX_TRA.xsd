	<xs:complexType name="RailStationSequenceType_TRA">
		<xs:annotation>
			<xs:documentation>台鐵站序描述資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Sequence" type="xs:int">
				<xs:annotation>
					<xs:documentation>站序描述(從1開始，整數遞增)</xs:documentation>
				</xs:annotation>
			</xs:element>
