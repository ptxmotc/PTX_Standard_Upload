	<!--=======[3rd Layer TRA Exclusive Data Type]=======-->
	<xs:complexType name="RailStationType_TRA">
		<xs:annotation>
			<xs:documentation>台鐵車站資料型別 </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="RailStationType">
				<xs:sequence>
					<xs:element name="StationClass" type="StationClassCodeEnum_TRA">
						<xs:annotation>
							<xs:documentation>車站等級</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RailTrainInfoType_TRA">
		<xs:annotation>
			<xs:documentation>台鐵車次資料型別 </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="RailTrainInfoType">
				<xs:sequence>
					<xs:element name="OverNightStationID" type="RailStationIDEnum_TRA">
						<xs:annotation>
							<xs:documentation>跨夜車站代碼</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TripLine" type="xs:string">
						<xs:annotation>
							<xs:documentation>山海線類型</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="WheelChairFlag" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>是否設身障旅客專用座位車</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PackageServiceFlag" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>是否提供行李服務</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DiningFlag" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>是否提供餐車服務</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BreastFeedingFlag" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>是否設有哺乳室</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BikeFlag" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>是否人車同行班次(置於攜車袋之自行車各級列車均可乘車)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CarFlag" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>是否提供小客車上車服務</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DailyFlag" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>是否每日行駛</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ExtraTrainFlag" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>是否為加班車</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Note" type="xs:string">
						<xs:annotation>
							<xs:documentation>附註說明</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RailFareType_TRA">
		<xs:annotation>
			<xs:documentation>台鐵收費描述資料型別</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="FareType">
				<xs:sequence>
					<xs:element name="TrainType" type="RailTrainTypeType">
						<xs:annotation>
							<xs:documentation>列車種類</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RailODFareType_TRA">
		<xs:annotation>
			<xs:documentation>台鐵起迄站收費資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OriginStation" type="RailStationType_TRA">
				<xs:annotation>
					<xs:documentation>起點車站資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DestinationStation" type="RailStationType_TRA">
				<xs:annotation>
					<xs:documentation>迄點車站資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TrainType" type="RailTrainTypeCodeEnum_TRA"/>
			<xs:element name="Direction" type="DirectionEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>方向性描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Fares">
				<xs:annotation>
					<xs:documentation>票價收費資訊</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Fare" type="RailFareType_TRA" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="TravelDistance" type="xs:double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>站間旅行參考距離(公里)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--=======[3rd Layer TRA Exclusive Dynamic Data Type ]=======-->
	<xs:complexType name="RailStationLiveBoardType_TRA">
		<xs:annotation>
			<xs:documentation>台鐵站別即時到離站資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="Station" type="RailStationType_TRA">
				<xs:annotation>
					<xs:documentation>車站資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TrainInfo" type="RailTrainInfoType_TRA">
				<xs:annotation>
					<xs:documentation>車次資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndingStation" type="RailStationType_TRA">
				<xs:annotation>
					<xs:documentation>終點車站資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TripLine" type="xs:string">
				<xs:annotation>
					<xs:documentation>山海線類型</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Platform" type="xs:string">
				<xs:annotation>
					<xs:documentation>停靠月台</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScheduledArrivalTime" type="xs:string">
				<xs:annotation>
					<xs:documentation>表訂到站時刻HH:mm</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScheduledDepartureTime" type="xs:string">
				<xs:annotation>
					<xs:documentation>表訂離站時刻HH:mm</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DelayTime" type="xs:integer">
				<xs:annotation>
					<xs:documentation>延誤分鐘</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RunningStatus" type="RailRunningStatusEnum">
				<xs:annotation>
					<xs:documentation>列車狀態</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UpdateTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>訊息更新時間</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RailTrainLiveBoardType_TRA">
		<xs:annotation>
			<xs:documentation>台鐵列車即時到離站資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TrainInfo" type="RailTrainInfoType_TRA">
