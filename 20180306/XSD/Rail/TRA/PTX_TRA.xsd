													<xs:documentation>車站出入口簡圖網址連結資訊
</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="ExitMapURL" type="MapURLType" maxOccurs="unbounded">
															<xs:annotation>
																<xs:documentation>車站出入口簡圖網址連結資訊
</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RailStationFacilityType_TRA">
		<xs:annotation>
			<xs:documentation>台鐵車站設施資料型別</xs:documentation>
		</xs:annotation>
