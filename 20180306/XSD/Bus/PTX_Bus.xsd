	<xs:complexType name="BusRouteType">
		<xs:annotation>
			<xs:documentation>公車路線資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RouteUID" type="xs:string">
				<xs:annotation>
					<xs:documentation>PTX路線獨一代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteID" type="xs:string">
				<xs:annotation>
					<xs:documentation>地區既用中之路線代號</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteName" type="NameType">
				<xs:annotation>
					<xs:documentation>路線名稱</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteLongName" type="NameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>路線長名稱</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteDescription" type="NameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>路線描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HasSubRoutes" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>是否有附屬路線(0=否;1=是)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Operators">
				<xs:annotation>
					<xs:documentation>營運業者資料</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Operator" type="OperatorType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="RouteType" type="BusRouteTypeEnum">
				<xs:annotation>
					<xs:documentation>路線主要類別代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ServiceType">
				<xs:annotation>
					<xs:documentation>公車路線服務類型</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="IsFreeBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為免費公車</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsTaiwanTripBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為台灣好行公車</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsTourBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為觀光巴士</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsTouristBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為遊覽車</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsBRTBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為BRT公車</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsMedicalBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為醫療公車</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsNightBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為夜間公車</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsTrunkBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為幹線公車</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsMetroShuttleBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為捷運接駁公車</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsTHSRShuttleBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為高鐵接駁公車</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsTRAShuttleBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為台鐵接駁公車</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsAirportShuttleBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為機場接駁公車</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="IsActivityShuttleBus" type="xs:boolean">
							<xs:annotation>
								<xs:documentation>是否為活動接駁公車</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="TicketPriceDescription" type="NameType">
				<xs:annotation>
					<xs:documentation>票價描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartureStopName" type="NameType">
				<xs:annotation>
					<xs:documentation>路線起點描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DestinationStopName" type="NameType">
				<xs:annotation>
					<xs:documentation>路線迄點描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FareBufferZoneDescription" type="NameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>計費緩衝區敘述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartStop" type="BusStopType">
				<xs:annotation>
					<xs:documentation>起點站牌</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndStop" type="BusStopType">
				<xs:annotation>
					<xs:documentation>迄點站牌</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsCircular" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>是否為環狀線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteURL" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>路線資訊說明網址</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LiveBusURL" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>路線公車動態資訊網址</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteMapImageURL" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>路線簡圖網址</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FareURL" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>收費資訊說明網址</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteDistance" type="xs:double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>公車營運路線長度(公里)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BusFirstLastTripInfoType">
		<xs:annotation>
			<xs:documentation>公車首末班車資訊資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Route" type="BusRouteType">
				<xs:annotation>
					<xs:documentation>路線資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SubRoute" type="BusSubRouteType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>路線資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Direction" type="DirectionEnum">
				<xs:annotation>
					<xs:documentation>方向性描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FirstLastTrips">
				<xs:annotation>
					<xs:documentation>首末班車資訊</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="FirstLastTrip" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ServiceDay" type="ServiceDayType">
										<xs:annotation>
											<xs:documentation>週內營運日</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="FirstTripDepTime" type="xs:string">
										<xs:annotation>
											<xs:documentation>頭班車發車時間HH:mm</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="LastTripDepTime" type="xs:string">
										<xs:annotation>
											<xs:documentation>末班車發車時間HH:mm</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BusRouteFareType">
		<xs:annotation>
			<xs:documentation>公車路線計費方式資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Route" type="BusRouteType">
				<xs:annotation>
					<xs:documentation>公車路線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Operator" type="OperatorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>營運業者</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SubRoute" type="BusSubRouteType">
				<xs:annotation>
					<xs:documentation>公車附屬路線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FarePricingType" type="FarePricingTypeEnum">
				<xs:annotation>
					<xs:documentation>描述該路線收費方式為:SectionFare/ODFare/StageFare</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsFreeBus" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>是否為免費路線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsForAllSubRoutes" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>該收費方式是否應用到所有附屬路線</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="SectionFares">
					<xs:annotation>
						<xs:documentation>段次計費</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SectionFare" type="BusSectionFareType" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ODFares">
					<xs:annotation>
						<xs:documentation>起迄站間計費</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ODFare" type="BusODFareType" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="StageFares">
					<xs:annotation>
						<xs:documentation>計費站區間計費</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="StageFare" type="BusStageFareType" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BusSubRouteType">
		<xs:annotation>
			<xs:documentation>公車附屬路線資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RouteUID" type="xs:string">
				<xs:annotation>
					<xs:documentation>唯一識別路線代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteID" type="xs:string">
				<xs:annotation>
					<xs:documentation>地區既用中之路線代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteName" type="NameType">
				<xs:annotation>
					<xs:documentation>路線名稱</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SubRouteUID" type="xs:string">
				<xs:annotation>
					<xs:documentation>PTX附屬路線獨一代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
