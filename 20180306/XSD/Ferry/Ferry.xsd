	<!--=======[2nd Layer FerryData Type]=======-->
	<xs:complexType name="PortType">
		<xs:annotation>
			<xs:documentation>航運港口資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PortID" type="xs:string">
				<xs:annotation>
					<xs:documentation>港口代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PortCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>港口簡碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PortName" type="NameType">
				<xs:annotation>
					<xs:documentation>港口名稱</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PortPosition" type="PointType">
				<xs:annotation>
					<xs:documentation>港口位置</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PortAddress" type="xs:string">
				<xs:annotation>
					<xs:documentation>港口地址</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PortPhone" type="xs:string">
				<xs:annotation>
					<xs:documentation>港口電話</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="City" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>港口所屬城市</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Country" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>港口所國家</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PortURL" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>港口官網網址</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FerryOperatorType">
		<xs:annotation>
			<xs:documentation>航運營運業者資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OperatorID" type="xs:string">
				<xs:annotation>
					<xs:documentation>營運業者代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OperatorName" type="NameType">
				<xs:annotation>
					<xs:documentation>營運業者名稱</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OperatorPhone">
				<xs:annotation>
					<xs:documentation>營運業者連絡電話</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[0][0-9]{1,2}-[0-9]{6,8}#?([0-9]*)?"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OperatorEmail">
				<xs:annotation>
					<xs:documentation>營運業者電子信箱</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value=".*@.*[.].*"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OperatorURL" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>營運業者網址鏈結</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FareURL" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>營運業者票價網址連結</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReservationURL" type="xs:anyURI" minOccurs="0">
				<xs:annotation>
					<xs:documentation>營運業者訂票網站</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReservationPhone" minOccurs="0">
				<xs:annotation>
					<xs:documentation>營運業者訂票電話</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[0][0-9]{1,2}-[0-9]{6,8}#?([0-9]*)?"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OperatorLogoURL" minOccurs="0">
				<xs:annotation>
					<xs:documentation>營運業者Logo網址</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:anyURI">
						<xs:pattern value="[0][0-9]{1,2}-[0-9]{6,8}#?([0-9]*)?"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FerryRouteType">
		<xs:annotation>
			<xs:documentation>航運航線資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RouteID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>航線代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteName" type="NameType">
				<xs:annotation>
					<xs:documentation>航線名稱</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Description" type="xs:string">
				<xs:annotation>
					<xs:documentation>航線描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Operator" type="OperatorType">
				<xs:annotation>
					<xs:documentation>航運業者資料</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartPort" type="PortType">
				<xs:annotation>
					<xs:documentation>航線起點港口資訊</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndPort" type="PortType">
				<xs:annotation>
					<xs:documentation>航線迄點港口資訊</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TicketPriceDescription" type="NameType">
				<xs:annotation>
					<xs:documentation>票價描述</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteType" type="FerryRouteTypeEnum">
				<xs:annotation>
					<xs:documentation>航線種類1:國際; 2:國內; 3:兩岸; 4:遊輪; 5:其他</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteMapURL" type="xs:anyURI">
				<xs:annotation>
					<xs:documentation>航運路線簡圖網址</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteDistance" type="xs:double">
				<xs:annotation>
					<xs:documentation>航線哩程</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FerryLegOfRouteType">
		<xs:annotation>
			<xs:documentation>航運航線航段資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FerryRouteLegID" type="xs:string">
				<xs:annotation>
					<xs:documentation>航線航段代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FerryRouteLegName" type="NameType">
				<xs:annotation>
					<xs:documentation>航線航段</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FromPort" type="PortType">
				<xs:annotation>
					<xs:documentation>航線航段起點港口資訊</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ToPort" type="PortType">
				<xs:annotation>
					<xs:documentation>航線航段迄點港口資訊</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FerryRouteDistance" type="xs:double">
				<xs:annotation>
					<xs:documentation>航線航段行駛哩程</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VesselType">
		<xs:annotation>
			<xs:documentation>航運船舶資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="VesselID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>船舶編號</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VesselName" type="NameType">
				<xs:annotation>
					<xs:documentation>船舶名稱</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Nationality" type="xs:string">
				<xs:annotation>
					<xs:documentation>船舶所屬國籍</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Operator" type="FerryOperatorType">
				<xs:annotation>
					<xs:documentation>營運業者</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FerryRouteFareType">
		<xs:annotation>
			<xs:documentation>航運票價資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Route" type="FerryRouteType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>船舶編號</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FareID" type="xs:string">
				<xs:annotation>
					<xs:documentation>票價代碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OriginPort" type="PortType">
				<xs:annotation>
					<xs:documentation>起始港口資訊</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DestinationPort" type="PortType">
				<xs:annotation>
					<xs:documentation>迄點港口資訊</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Fares">
				<xs:annotation>
					<xs:documentation>票價內容</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Fare" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="FareName" type="xs:string">
										<xs:annotation>
											<xs:documentation>票種名稱</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="TicketType" type="TicketTypeEnum">
										<xs:annotation>
											<xs:documentation>票種類型(例如:一般票、來回票、電子票證、回數票、定期票、早鳥票等)</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="FareClass" type="FareClassEnum">
										<xs:annotation>
											<xs:documentation>費率等級(例如:成人、學生、孩童、敬老等)</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="CabinClass" type="xs:integer">
										<xs:annotation>
											<xs:documentation>艙等1.標準座車廂
2.商務座車廂
3.自由座車廂</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Price" type="xs:integer">
										<xs:annotation>
											<xs:documentation>計費價格(新台幣)</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GeneralFerryScheduleType">
		<xs:annotation>
			<xs:documentation>航運定期班表資料型別</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ScheduleStartDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>班表開始日期</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScheduleEndDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>班表結束日期</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Operator" type="OperatorType">
				<xs:annotation>
					<xs:documentation>航運業者資訊</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Vessel" type="VesselType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>船舶號碼</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeparturePort" type="PortType">
				<xs:annotation>
					<xs:documentation>離港港口</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartureTime" type="xs:time">
				<xs:annotation>
					<xs:documentation>離港時間</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransitPorts" minOccurs="0">
				<xs:annotation>
					<xs:documentation>中繼港口資訊</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TransitPort">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="FerryTransitType"/>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ArrivalPort" type="PortType">
				<xs:annotation>
					<xs:documentation>進港港口</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArrivalTime" type="xs:time">
				<xs:annotation>
					<xs:documentation>進港時間</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Monday" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>週一航駛與否</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Tuesday" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>週二航駛與否</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Wednesday" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>週三航駛與否</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Thursday" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>週四航駛與否</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Friday" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>週五航駛與否</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Saturday" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>週六航駛與否</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Sunday" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>週日航駛與否</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteType" type="FerryRouteTypeEnum">
				<xs:annotation>
					<xs:documentation>航線種類  1:國際; 2:國內; 3:兩岸; 4:遊輪; 5:其他</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MonthlyFerryScheduleType">
		<xs:annotation>
			<xs:documentation>航運月航行班表資料型別</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="GeneralFerryScheduleType"/>
