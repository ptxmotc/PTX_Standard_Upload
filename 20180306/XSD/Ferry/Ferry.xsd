					<xs:documentation>中繼資料次序(由1開此)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransitPort" type="PortType">
				<xs:annotation>
					<xs:documentation>中繼港口</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransitCause" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>中繼原因</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ArrivalTime" type="xs:time">
				<xs:annotation>
					<xs:documentation>中繼港口離港時間</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartureTime" type="xs:time">
				<xs:annotation>
					<xs:documentation>中繼港口進港時間</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
