<xs:schema xmlns="http://ptx.transportdata.tw/standard/schema/" xmlns:editix="http://ptx.transportdata.tw/standards/schema/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://ptx.transportdata.tw/standard/schema/" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="2.0" id="PTX_Ferry">
	<!--=======[Dependencies]=======-->
	<xs:include schemaLocation="../PTX_Common.xsd"/>
