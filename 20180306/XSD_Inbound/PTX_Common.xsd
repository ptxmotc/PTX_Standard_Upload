	<xs:simpleType name="AirFlightTypeEnum">
		<xs:annotation>
			<xs:documentation>[航空運具航班種類分類代碼]
			========
			Air:01-09     
			01國際 
			02國內 
			03兩岸 
			04國際包機 
			05國內包機 
			06兩岸包機
			07貨機 
			08-09特殊(保留)
 </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-0][1-9]"/>
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="6"/>
			<xs:enumeration value="-2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RailRouteTypeEnum">
		<xs:annotation>
			<xs:documentation>[軌道運具路線種類分類代碼]
			=======
			Rail:31-49
			 31傳統鐵路(Inter Regional Rail)
			32高速鐵路(High Speed Rail)
			33都市捷運(MRT)
			34機場捷運(Airport-Link MRT)
			35城市輕軌(Light Rail)
			36登山鐵路(Funicular)
			40-49 特殊(保留)
 </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[3-4][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
